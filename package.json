{
  "name": "sat-frontend",
  "private": true,
  "description": "frontend of https://sat-dillenburg.de",
  "version": "0.1.1",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf public/ .cache/ .linaria-cache/ && gatsby build",
    "build:disclaimer": "yarn licenses generate-disclaimer --production=true > DISCLAIMER.md",
    "develop": "gatsby develop -p 8080",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "yarn run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "type-check": "tsc --noEmit",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --fix"
  },
  "dependencies": {
    "gatsby": "^2.24.54",
    "gatsby-image": "^2.4.19",
    "html-entities": "^1.3.1",
    "hyphen": "^1.6.2",
    "linaria": "2.0.2",
    "moment": "^2.29.1",
    "polished": "^3.6.7",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@directus/sdk-js": "^6.3.0",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "axios": "^0.20.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "gatsby-plugin-feed": "^2.5.14",
    "gatsby-plugin-linaria": "^2.1.0",
    "gatsby-plugin-react-helmet": "^3.3.11",
    "gatsby-plugin-sharp": "^2.6.36",
    "gatsby-source-filesystem": "^2.3.30",
    "gatsby-transformer-sharp": "^2.5.15",
    "node-vibrant": "^3.1.5",
    "prettier": "^2.1.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sat-dillenburg/frontend"
  }
}
